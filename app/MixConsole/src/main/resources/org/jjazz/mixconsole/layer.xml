<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <!-- Declare the folders order -->
    <folder name="Actions">
        <folder name="MixConsole">
            <folder name="MenuBar">
                <folder name="File">                                     
                    <attr name="displayName" bundlevalue="org.jjazz.mixconsole.Bundle#MixConsoleMenuBarFile"/>
                    <attr intvalue="100" name="position"/>
                </folder>
                <folder name="Edit">
                    <attr name="displayName" bundlevalue="org.jjazz.mixconsole.Bundle#MixConsoleMenuBarEdit"/>                    
                    <attr intvalue="200" name="position"/>
                </folder>
                <folder name="Midi">
                    <attr intvalue="300" name="position"/>
                </folder>
            </folder>
        </folder>
    </folder>
</filesystem>
