<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    
    <folder name="Actions">
        <folder name="Edit">
            <file name="org-openide-actions-FindAction.instance_hidden"/>
        </folder>
        <folder name="File"/>
        <folder name="Window">
            <file name="org-netbeans-core-windows-actions-DockModeAction.instance_hidden"/> 
            <file name="org-netbeans-core-windows-actions-MinimizeModeAction.instance_hidden"/>
            <file name="org-netbeans-core-windows-actions-MoveModeAction.instance_hidden"/>
            <file name="org-netbeans-core-windows-actions-MoveWindowAction.instance_hidden"/>
            <file name="org-netbeans-core-windows-actions-ResizeModeAction.instance_hidden"/>
            <file name="org-netbeans-core-windows-actions-UndockModeAction.instance_hidden"/>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Edit">
            <file name="org-openide-actions-DeleteAction.shadow_hidden"/>
            <file name="org-openide-actions-FindAction.shadow_hidden"/>
            <file name="org-openide-actions-ReplaceAction.shadow_hidden"/>
        </folder>
        <folder name="File">
            <file name="org-netbeans-modules-print-action-PageSetupAction.shadow_hidden"/>
            <file name="org-netbeans-modules-print-action-PrintAction.shadow_hidden"/>
            <file name="org-openide-actions-SaveAction.shadow_hidden"/>
            <file name="org-openide-actions-SaveAllAction.shadow_hidden"/>
            <file name="org-openide-actions-SaveAsAction.shadow_hidden"/>
        </folder>
        <file name="GoTo_hidden"/>
        <folder name="Help">
            <file name="org-netbeans-core-actions-AboutAction.shadow_hidden"/>
            <file name="org-netbeans-modules-autoupdate-ui-actions-CheckForUpdatesAction.shadow_hidden"/>
        </folder>
        <folder name="Tools">
            <file name="Separator1.instance_hidden"/>
            <file name="Separator2.instance_hidden"/>
            <file name="org-netbeans-modules-autoupdate-ui-actions-PluginManagerAction.shadow_hidden"/>
            <file name="org-netbeans-modules-options-OptionsWindowAction-separatorBefore.instance_hidden"/>
            <file name="org-netbeans-modules-templates-actions-TemplatesAction.shadow_hidden"/>
        </folder>
        <file name="View_hidden"/>
        <folder name="Window">
            <file name="ConfigureWindow_hidden"/>
            <file name="DocumentsAction.shadow_hidden"/>
            <file name="GroupsMenuAction.shadow_hidden"/>
            <file name="Separator3.instance_hidden"/>
            <file name="SwitchToRecentDocumentAction.shadow_hidden"/>
            <file name="ToggleFullScreenAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-netbeans-core-windows-actions-ToggleFullScreenAction.instance"/>
                <attr name="position" intvalue="100"/>
            </file>
            <file name="Tools_hidden"/>
            <file name="Web_hidden"/>
            <file name="org-jjazz-improvisionsupport-api-ImproSupportTopComponent-separatorAfter.instance_hidden"/>
            <file name="org-netbeans-core-io-ui-IOWindowAction.shadow_hidden"/>
            <file name="org-netbeans-core-windows-actions-ResetWindowsAction.shadow_hidden"/>
            <file name="org-netbeans-modules-favorites-View.shadow_hidden"/>
        </folder>
    </folder>
    <folder name="OptionsDialog">
        <file name="Advanced.instance_hidden"/>
        <file name="Appearance.instance_hidden"/>
        <file name="General.instance_hidden"/>
        <file name="Keymaps.instance_hidden"/>
    </folder>
    <!-- the  OptionsExport folder makes Import/Export buttons in the Options Dialog disappear (since there is no more setting candidates for import/export).
    See org.netbeans.modules.options.OptionsDialogDisplayerImpl.java for details -->
    <file name="OptionsExport_hidden"/>
    <folder name="Services">
        <folder name="AutoupdateType">
            <file name="org_jjazz_base_update_center.instance">
                <attr name="displayName" bundlevalue="org.jjazz.base.Bundle#Services/AutoupdateType/org_jjazz_base_update_center.instance"/>
                <attr name="enabled" boolvalue="true"/>
                <attr name="instanceCreate" methodvalue="org.netbeans.modules.autoupdate.updateprovider.AutoupdateCatalogFactory.createUpdateProvider"/>
                <attr name="instanceOf" stringvalue="org.netbeans.spi.autoupdate.UpdateProvider"/>
                <attr name="url" bundlevalue="org.jjazz.base.Bundle#org_jjazz_base_update_center"/>
            </file>
        </folder>
    </folder>
    <folder name="Toolbars">
        <file name="Standard.xml" url="Standard.xml"/>                    
        <!-- Need to specify position of our toolbars (despite this is also done in Standard.xml), otherwise warning "Not all children in Toolbars/ marked with the position attribute..." -->
        <folder name="ControlToolbar">
            <attr name="position" intvalue="400"/>  
        </folder>
        <folder name="AutoPreviewToolbar">
            <attr name="position" intvalue="450"/> 
        </folder> 
        <folder name="File" >
            <!-- We don't use the IDE action but our own  -->
            <file name="org-openide-actions-SaveAllAction.shadow_hidden"/>
        </folder>   
    </folder>
    <folder name="Shortcuts">
        <!-- Add shortcuts for Netbeans actions -->
        <file name="O-O.shadow">
            <attr name="originalFile" stringvalue="Actions/Window/org-netbeans-modules-options-OptionsWindowAction.instance"/>
        </file>
        <!-- Delegate actions -->        
        <file name="D-Z.shadow">
            <attr name="originalFile" stringvalue="Actions/Edit/org-openide-actions-UndoAction.instance"/>
        </file>
        <file name="D-Y.shadow">
            <attr name="originalFile" stringvalue="Actions/Edit/org-openide-actions-RedoAction.instance"/>
        </file>
        <file name="D-C.shadow">
            <attr name="originalFile" stringvalue="Actions/Edit/org-openide-actions-CopyAction.instance"/>
        </file>        
        <file name="D-X.shadow">
            <attr name="originalFile" stringvalue="Actions/Edit/org-openide-actions-CutAction.instance"/>
        </file>     
        <file name="D-V.shadow">
            <attr name="originalFile" stringvalue="Actions/Edit/org-openide-actions-PasteAction.instance"/>
        </file>         
    </folder>        
        
    <folder name="Windows2">
        <folder name="Modes">
            <folder name="jlnavigator"/>
            <file name="jlnavigator.wsmode" url="modes/jlnavigator.wsmode"/>
            <folder name="midieditor"/>
            <file name="midieditor.wsmode" url="modes/midieditor.wsmode"/>
        </folder>
    </folder>    
    
    
    <!--
        Required since NB28 because of https://github.com/apache/netbeans/pull/8908
        Branding for Favorites module preferences in order to avoid the automatic opening of the favorites tab.
        Mirrors: config/Preferences/org/netbeans/modules/favorites.properties with a flag that disables the feature.
    -->
    <folder name="Preferences">
        <folder name="org">
            <folder name="netbeans">
                <folder name="modules">
                    <file name="favorites.properties" url="favorites.properties"/>
                </folder>
            </folder>
        </folder>
    </folder>
    
</filesystem>
